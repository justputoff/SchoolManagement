{
    "info": {
      "name": "Student API",
      "_postman_id": "12345-67890-abcdef",
      "description": "API untuk mengelola data student",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Login",
        "request": {
          "method": "POST",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\n    \"email\": \"user@example.com\",\n    \"password\": \"password\"\n}"
          },
          "url": {
            "raw": "http://intens.edupay.cloud/login",
            "host": ["intens.edupay.cloud"],
            "path": ["login"]
          }
        }
      },
      {
        "name": "Logout",
        "request": {
          "method": "POST",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/json"
            },
            {
              "key": "Authorization",
              "value": "Bearer {{token}}"
            }
          ],
          "url": {
            "raw": "http://intens.edupay.cloud/logout",
            "host": ["intens.edupay.cloud"],
            "path": ["logout"]
          }
        }
      },
      {
        "name": "Get Users",
        "request": {
          "method": "GET",
          "header": [
            {
              "key": "Authorization",
              "value": "Bearer {{token}}"
            }
          ],
          "url": {
            "raw": "http://intens.edupay.cloud/users",
            "host": ["intens.edupay.cloud"],
            "path": ["users"]
          }
        }
      },
      {
        "name": "Get User",
        "request": {
          "method": "GET",
          "header": [
            {
              "key": "Authorization",
              "value": "Bearer {{token}}"
            }
          ],
          "url": {
            "raw": "http://intens.edupay.cloud/user",
            "host": ["intens.edupay.cloud"],
            "path": ["user"]
          }
        }
      },
      {
        "name": "Fetch User",
        "request": {
          "method": "POST",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/json"
            },
            {
              "key": "Authorization",
              "value": "Bearer {{token}}"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\n    \"criteria\": \"value\"\n}"
          },
          "url": {
            "raw": "http://intens.edupay.cloud/user/fetch",
            "host": ["intens.edupay.cloud"],
            "path": ["user", "fetch"]
          }
        }
      },
      {
        "name": "Get User Detail",
        "request": {
          "method": "GET",
          "header": [
            {
              "key": "Authorization",
              "value": "Bearer {{token}}"
            }
          ],
          "url": {
            "raw": "http://intens.edupay.cloud/user/detail",
            "host": ["intens.edupay.cloud"],
            "path": ["user", "detail"]
          }
        }
      },
      {
        "name": "Update Student",
        "request": {
          "method": "PUT",
          "header": [
            {
              "key": "Content-Type",
              "value": "application/json"
            },
            {
              "key": "Authorization",
              "value": "Bearer {{token}}"
            }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\n    \"address\": \"new address\",\n    \"phone\": \"123456789\",\n    \"cabang\": \"new cabang\"\n}"
          },
          "url": {
            "raw": "http://intens.edupay.cloud/student/:id",
            "host": ["intens.edupay.cloud"],
            "path": ["student", ":id"]
          }
        }
      }
    ]
